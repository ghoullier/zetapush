{
  "name": "@zetapush/troubleshooting",
  "version": "0.37.7",
  "description": "Zeta Push Next",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "lib",
    "es",
    "LICENSE"
  ],
  "repository": "https://github.com/zetapush/zetapush",
  "author": "ci@zetapush.com",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib dist es .deploy .esdoc",
    "build:commonjs": "tsc --outDir lib",
    "build:es": "tsc --module ES2015 --moduleResolution Node --outDir es",
    "build": "npm run build:commonjs && npm run build:es",
    "prepare": "npm run clean && npm run build",
    "test:ci+coverage": "rimraf coverage .nyc_output && nyc node_modules/.bin/ts-node src/index.ts"
  },
  "dependencies": {
    "@zetapush/common": "0.37.7",
    "axios": "0.18.0",
    "default-gateway": "3.0.1",
    "execa": "1.0.0",
    "ora": "3.0.0"
  },
  "devDependencies": {
    "@types/node": "10.12.9",
    "cross-env": "5.2.0",
    "nyc": "13.1.0",
    "rimraf": "2.6.2",
    "rollup": "0.67.3",
    "rollup-plugin-bundle-size": "1.0.2",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-json": "3.1.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-terser": "3.0.0",
    "rollup-plugin-typescript": "1.0.0",
    "rollup-watch": "4.3.1",
    "ts-node": "7.0.1",
    "typescript": "3.1.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov",
      "html",
      "cobertura",
      "text"
    ],
    "all": true
  }
}
